{
    "name":"Test Method 2",
    "ver":"1.0.1",
    "authors": [ "msneddon","wstester1" ],
    "contact": "help@kbase.us",
    
    "categories": [ "testmethods" ],
    
    "widgets": {
        "output":"KBaseDefaultViewer"
    },
    
    "parameters": [
        {
            "id": "genome",
            
            "optional":false,
            "advanced":false,
            
            "allow_multiple":false,
            "default_values":[""],
            
            "field_type" : "text",
            
            "text_options":{
                "valid_ws_types":["KBaseGenomes.Genome", "KBaseGenomes.PlantGenome"]
            }
        },
        
        {
            "id": "iterations",
            
            "optional":false,
            "advanced":true,
            
            "allow_multiple":false,
            "default_values":["1"],
            
            "field_type" : "intslider",
            
            "intslider_options":{
                "min":1,
                "max":10,
                "step":1
            }
        }
    ],
    
    "behavior": {
        "service-mapping": {
            "url": "${endpoint-host}:555${endpoint-base}/neverservice",
            "name": "KBaseNeverService",
            "method": "test_method",
            "input_mapping": [
                { 
                    "input_parameter": "genome",
                    "generated_value": {
                        "prefix": "genome_",
                        "symbols": 8,
                        "suffix": ".obj"
                    },
                    "target_property": "input_genome",
                    "target_type_transform": "ref"
                },
                { 
                    "narrative_system_variable": "workspace",
                    "target_argument_position": 0,
                    "target_property": "input_workspace"
                }
            ],
            "output_mapping": [
                { 
                    "constant_value": [0, "1", 2.0],
                    "target_property": "ret1"
                },
                { 
                    "service_method_output_path": ["key1", "key2"],
                    "target_property": "re2"
                }
            ]
        }
    },
    "job_id_output_field": "re2"
}
